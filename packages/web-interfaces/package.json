{
    "name": "@napgram/web-interfaces",
    "version": "0.1.4",
    "description": "Shared web route modules for NapGram",
    "main": "./src/index.ts",
    "types": "./dist/src/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "import": "./src/index.ts",
            "default": "./src/index.ts"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "dev": "tsc -p tsconfig.json --watch"
    },
    "dependencies": {
        "@kitajs/html": "^4.2.11",
        "@napgram/auth-kit": "workspace:*",
        "@napgram/message-kit": "workspace:*",
        "@napgram/plugin-kit": "workspace:*",
        "@napgram/telegram-client": "workspace:*",
        "fastify": "5.6.2",
        "zod": "4.3.5"
    },
    "keywords": [
        "napgram",
        "web",
        "interfaces"
    ],
    "author": "NapGram Team",
    "license": "MIT",
    "devDependencies": {
        "@fastify/cookie": "^11.0.2",
        "@kitajs/ts-html-plugin": "^4.1.3",
        "@napgram/infra-kit": "workspace:*",
        "@napgram/runtime-kit": "workspace:*",
        "@types/node": "25.0.3",
        "typescript": "5.9.3"
    },
    "publishConfig": {
        "main": "./dist/src/index.js",
        "types": "./dist/src/index.d.ts",
        "exports": {
            ".": {
                "types": "./dist/src/index.d.ts",
                "import": "./dist/src/index.js",
                "default": "./dist/src/index.js"
            },
            "./package.json": "./package.json"
        }
    }
}
