{
    "name": "@napgram/plugin-adapter-telegram-mtcute",
    "version": "1.0.0",
    "description": "NapGram Telegram adapter for mtcute",
    "main": "./dist/index.js",
            "types": "./src/index.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "dev": "tsc -p tsconfig.json --watch"
    },
    "keywords": [
        "napgram",
        "plugin",
        "adapter",
        "telegram",
        "mtcute"
    ],
    "author": "NapGram Team",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^25.0.3",
        "typescript": "^5.9.3",
        "@napgram/sdk": "workspace:*"
    },
    "dependencies": {
        "@napgram/telegram-client": "workspace:*"
    }
}
