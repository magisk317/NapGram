{
    "name": "@napgram/qq-client",
    "version": "0.1.4",
    "description": "Shared QQ client for NapGram",
    "main": "./src/index.ts",
    "types": "./dist/src/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "import": "./src/index.ts",
            "default": "./src/index.ts"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "tsc -b",
        "dev": "tsc -b -w",
        "test": "vitest run --passWithNoTests"
    },
    "keywords": [
        "napgram",
        "qq",
        "client"
    ],
    "author": "NapGram Team",
    "license": "MIT",
    "dependencies": {
        "@naplink/naplink": "^0.0.3"
    },
    "devDependencies": {
        "@types/node": "25.0.3",
        "typescript": "5.9.3",
        "vitest": "4.0.16"
    },
    "publishConfig": {
        "main": "./dist/src/index.js",
        "types": "./dist/src/index.d.ts",
        "exports": {
            ".": {
                "types": "./dist/src/index.d.ts",
                "import": "./dist/src/index.js",
                "default": "./dist/src/index.js"
            },
            "./package.json": "./package.json"
        }
    }
}
