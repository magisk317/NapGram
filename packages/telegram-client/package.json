{
    "name": "@napgram/telegram-client",
    "version": "0.1.4",
    "description": "Shared Telegram client for NapGram",
    "main": "./src/index.ts",
    "types": "./dist/src/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "import": "./src/index.ts",
            "default": "./src/index.ts"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "tsc -b",
        "dev": "tsc -b -w"
    },
    "keywords": [
        "napgram",
        "telegram",
        "client"
    ],
    "author": "NapGram Team",
    "license": "MIT",
    "dependencies": {
        "@mtcute/core": "^0.27.5",
        "@mtcute/dispatcher": "^0.27.5",
        "@mtcute/node": "^0.27.5"
    },
    "devDependencies": {
        "@types/node": "25.0.3",
        "typescript": "5.9.3"
    },
    "publishConfig": {
        "main": "./dist/src/index.js",
        "types": "./dist/src/index.d.ts",
        "exports": {
            ".": {
                "types": "./dist/src/index.d.ts",
                "import": "./dist/src/index.js",
                "default": "./dist/src/index.js"
            },
            "./package.json": "./package.json"
        }
    }
}
